#!/usr/bin/env bash

DIR_BIN="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
DIR_PROJECT="$DIR_BIN/.."
DIR_BUILD="$DIR_PROJECT/build"
DIR_OUTPUTS="$DIR_BUILD/output"

"$DIR_BIN/gradlew" --quiet ":cli:installDist" || exit 1
"$DIR_PROJECT/cli/build/install/rv-cli/bin/rv-cli" decrypt "$1" "$2" || exit 1
